{
  "name": "campaign-bay",
  "version": "0.0.9",
  "description": "CampaignBay for WooCommerce Product Discount WordPress Plugin.",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "browsersync": "browser-sync start --proxy 'localhost' --files 'build/*.js, build/*.css, **/*.php' --no-notify",
    "dev": "npm-run-all --parallel --print-label browsersync start",
    "makepot": "wp i18n make-pot . languages/campaignbay.pot --domain=campaignbay --slug=campaignbay --exclude='node_modules,vendor,src'",
    "make-json": "wp i18n make-json languages --no-purge && node helper/rename-translations.js"
  },
  "author": "sankarsan das",
  "license": "GPLv2 or later",
  "devDependencies": {
    "@wordpress/babel-plugin-makepot": "^6.26.0",
    "@wordpress/scripts": "^30.19.0",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@wordpress/dataviews": "^4.22.0",
    "@wordpress/date": "^5.28.0",
    "@wordpress/i18n": "^5.26.0",
    "@wordpress/icons": "^10.26.0",
    "@wordpress/react-i18n": "^4.26.0",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.525.0",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.6.3"
  },
  "wp-i18n": {
    "domain": "wpab-cb",
    "potFile": "languages/wpab-cb.pot"
  }
}
